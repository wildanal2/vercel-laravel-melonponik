{
    "version": 2,
    "framework": null,
    "github": {
        "enabled": false
    },
    "builds": [
        {
            "src": "/api/index.php",
            "use": "vercel-php@0.7.4"
        },
        {
            "src": "/public/build/assets/**",
            "use": "@vercel/static"
        },
        {
            "src": "/public/**",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/build/assets/(.*)",
            "dest": "/public/build/assets/$1"
        },
        {
            "src": "/assets/(.*)",
            "dest": "/public/assets/$1"
        },
        {
            "src": "/favicon.ico",
            "dest": "/public/favicon.ico"
        },
        {
            "src": "/og-image.jpg",
            "dest": "/public/og-image.jpg"
        },
        {
            "src": "/favicon-48x48.png",
            "dest": "/public/favicon-48x48.png"
        },
        {
            "src": "/robots.txt",
            "dest": "/public/robots.txt"
        },
        {
            "src": "/build/manifest.json",
            "dest": "/public/build/manifest.json"
        },
        {
            "src": "/(.*)",
            "dest": "/api/index.php"
        }
    ],
    "outputDirectory": "public",
    "env": {
        "APP_ENV": "production",
        "APP_DEBUG": "true",
        "APP_NAME": "MelonPonik",
        "APP_URL": "https://melonponik.vercel.app",
        "ASSET_URL": "https://melonponik.vercel.app",
        "APP_KEY": "base64:SHvmxrgeyFlxPuwJdVXHEarXK5gkuRcR2DhZKBWBTTk=",
        "APP_CONFIG_CACHE": "/tmp/config.php",
        "APP_EVENTS_CACHE": "/tmp/events.php",
        "APP_PACKAGES_CACHE": "/tmp/packages.php",
        "APP_ROUTES_CACHE": "/tmp/routes.php",
        "APP_SERVICES_CACHE": "/tmp/services.php",
        "VIEW_COMPILED_PATH": "/tmp",
        "CACHE_DRIVER": "array",
        "LOG_CHANNEL": "stderr",
        "SESSION_DRIVER": "cookie",
        "LOG_DEPRECATIONS_CHANNEL": "null",
        "LOG_LEVEL": "debug",
        "BROADCAST_DRIVER": "log",
        "FILESYSTEM_DISK": "local",
        "QUEUE_CONNECTION": "sync",
        "SESSION_LIFETIME": "120",
        "MEMCACHED_HOST": "127.0.0.1",
        "REDIS_HOST": "127.0.0.1",
        "REDIS_PASSWORD": "null",
        "REDIS_PORT": "6379",
        "MAIL_MAILER": "smtp",
        "MAIL_HOST": "mailpit",
        "MAIL_PORT": "1025",
        "MAIL_USERNAME": "null",
        "MAIL_PASSWORD": "null",
        "MAIL_ENCRYPTION": "null",
        "MAIL_FROM_ADDRESS": "hello@example.com",
        "MAIL_FROM_NAME": "Laracel-mailer"
    }
}
